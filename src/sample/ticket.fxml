<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Ticket">
    <children>
        <Button fx:id="ticketButton" layoutX="22.0" layoutY="97.0" mnemonicParsing="false" onAction="#ticketButtonClicked" text="Submit" AnchorPane.leftAnchor="22.0" AnchorPane.topAnchor="97.0" />
        <TextField layoutX="22.0" layoutY="45.0" promptText="Email" AnchorPane.leftAnchor="22.0" AnchorPane.topAnchor="45.0" />
        <TextField layoutX="200.0" layoutY="45.0" promptText="Ticket-id" AnchorPane.leftAnchor="200.0" AnchorPane.topAnchor="45.0" />
    </children>
</AnchorPane>
